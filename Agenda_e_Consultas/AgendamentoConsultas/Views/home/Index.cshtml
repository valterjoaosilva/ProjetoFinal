
@{

    List<SelectListItem> medicos = new List<SelectListItem>();
    medicos.Add(new SelectListItem {Text = "Dirceu", Value = "0" });
    medicos.Add(new SelectListItem { Text = "Valter", Value = "1" });
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Login</title>
</head>
<body>
    <div>
        <h2>CPF</h2>
        <input id="Cpf"></input>

        <h2>Nome</h2>
        <input id="Nome"></input> 

        <button>Logar</button>
        <button>Cadastrar</button>

        <h1>medicos</h1>
        @Html.DropDownList("Médicos", medicos, "Escolha seu médico");
    </div>
</body>
</html>
